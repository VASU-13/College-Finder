<% layout('layout') -%>

<% if (data.length === 0) { %>
    <h3>Search not found</h3>
<% } else {%>
    <h3>Results for <%=  query %></h3>
    <div class="container">
    <div class="row">
    <% for (var i=0;i< data.length;i++) { %>
        <div class="col-md-4">
            <a href="/college/<%= data[i]._source._id %>">
                <div class="thumbnail">
                    <div class="caption">
                        <h3><%= data[i]._source.collegename %></h3>
                        <h3><%= data[i]._source.rtitle %></h3>
                    </div>
                </div>
            </a>
        </div>
        <% } %>
    </div>
    </div>
    <% } %>
